<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/darkGreen"></solid>
            <padding android:bottom="7dp"
                android:top="7dp"
                android:left="7dp"
                android:right="7dp"></padding>
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/darkestGreen"></solid>
        </shape>
    </item>
    <item android:bottom="2dp"
        android:right="2dp">
        <shape android:shape="oval">
            <solid android:color="@color/darkerGreen"></solid>
        </shape>
    </item>

</layer-list>